<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout">
    <layout:VerticalLayout>
        <core:Icon
            src="sap-icon://edit"
            class="sapUiTinyMargin"
            tooltip="Edit"
            press="handler.onEditPressed"/>
        <core:Icon
            src="sap-icon://delete"
            class="sapUiTinyMargin"
            tooltip="Delete"
            press="handler.onDeletePressed"/>
    </layout:VerticalLayout>
</core:FragmentDefinition>